{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "constant": [
        "./constants"
      ],
      "layout": [
        "./views/Layout"
      ],
      "utils": [
        "./utils"
      ],
    },
    "esModuleInterop": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}